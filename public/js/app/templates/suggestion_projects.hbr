<div class="col-xs-4 col-md-2">
	<h5>Projects</h5>
</div>
	<div class="col-xs-8 col-md-10">
		<ul class="list-unstyled">
		{{#each .}}
		<li data-id="{{id}}"> <div class="title">{{title}}</div> 
			<div class="description">{{description}}
		{{#if sponsor}}
			<span class="sponsor">
			- sponsed by {{sponsor}}
		</span>
		{{/if}}
		</div>
	</li>
	{{/each}}
</ul>
<button type="button" class="btn btn-default btn-xs more-projects">Show more</button>
</div>